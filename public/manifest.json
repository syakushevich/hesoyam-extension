{
  "manifest_version": 3,
  "name": "Interview Assistant",
  "version": "1.0",
  "description": "Overlay interface like Render Square",
  "permissions": ["scripting"],
  "host_permissions": ["https://meet.google.com/*"],
  "background": {
    "service_worker": "assets/background.js"
  },
  "action": {
    "default_title": "Show Overlay"
  },
  "icons": {
    "16": "favicon.png",
    "32": "favicon.png",
    "48": "favicon.png",
    "128": "favicon.png"
  },
  "web_accessible_resources": [
    {
      "resources": ["assets/*"],
      "matches": ["https://meet.google.com/*"]
    }
  ]
}
